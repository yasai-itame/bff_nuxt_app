{
  "version": 2,

  "builds": [
    {
      "src": "./dist/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "build": {
    "env": {
      "DEBUG": "true"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./dist/src/main.js"
    }
  ],
  "buildCommand": "npm run vercel-build"
} 